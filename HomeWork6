my_dict = {
    "Valya": 2001,
    "kostya": 2004,
    "stepa": 1999
}
print(my_dict)
print(my_dict.get("kostya"))
my_dict.update({
    "Dima": 2010,
    "Kristina": 1998
})
my_dict.pop("kostya")
print(my_dict)
my_set = {1, 3, 4, 1, 2, 6, 2, 6, 1, "Taras"}
print(my_set)
my_set.add(21343)
my_set.add(3.1)
my_set.remove(1)
print(my_set)
